package GalacticCats;

import java.awt.Graphics;
import java.awt.Image;

import javax.swing.ImageIcon;
import javax.swing.JButton;
import javax.swing.JLabel;
import javax.swing.JPanel;

public class MarineCat extends JPanel {
	Image StartBackground=new ImageIcon("src/Image/startpanel.png").getImage();
	JLabel subMarine =new JLabel(new ImageIcon("src/Image/cat.png"));
	JButton startButton =new JButton();
	JButton loginButton =new JButton();
	
	boolean start=false;
	public MarineCat() {
		setupGUI();
	}
	public void setupGUI() {
		setBounds(0,0,1100,636);
		setLayout(null);
		subMarine.setBounds(0,0,60,60);
		add(subMarine);
	}
	public void paintComponent(Graphics g) {
		super.paintComponent(g);
		g.drawImage(StartBackground,0,0,null);
		
		this.repaint();
	}
	
	
	
	
	
	
}
